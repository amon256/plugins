<?xml version="1.0" encoding="UTF-8"?>
<applications charset="UTF-8">
	<application>
		<id>winplan2</id>
		<name>百姓分享平台</name>
		<appRoot>/usr/local/wars</appRoot>		
		<configRoot>/usr/local/tomcat8181/bin</configRoot>		
		<statusCmd>
			<cmd>/usr/local/status/winplan.sh</cmd>
			<include>tomcat</include>
		</statusCmd>
		<startCmd>
			<path>/usr/local/tomcat8080/bin</path>
			<cmd>./startup.sh</cmd>
		</startCmd>
		<stopCmd>
			<path>/usr/local/tomcat8080/bin</path>
			<cmd>./shutdown.sh</cmd>
		</stopCmd>
	</application>
</applications>
